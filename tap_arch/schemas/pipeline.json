{
  "properties": {
    "id": {
      "type": "string",
      "maxLength": 17,
      "minLength": 17,
      "pattern": "^pipeline-[a-zA-Z0-9]{8}$",
      "format": "arch-pipeline-id",
      "title": "Id",
      "description": "Arch pipeline ID.",
      "examples": [
        "pipeline-abcdef12",
        "pipeline-tarch230",
        "pipeline-3me1ty44"
      ]
    },
    "name": {
      "type": "string",
      "title": "Name"
    },
    "extractor_id": {
      "type": "string",
      "title": "extractor_id"
    },
    "database_for_extractor": {
      "anyOf": [
        {
          "type": "string",
          "maxLength": 11,
          "minLength": 11,
          "pattern": "^db-[a-z0-9]{8}$",
          "format": "arch-db-id",
          "description": "Arch database ID.",
          "examples": ["db-abcdef12", "db-tarch230", "db-3me1ty44"]
        },
        {
          "type": "null"
        }
      ],
      "title": "Database For Extractor"
    },
    "loader_id": {
      "type": "string",
      "title": "loader_id"
    },
    "database_for_loader": {
      "anyOf": [
        {
          "type": "string",
          "maxLength": 11,
          "minLength": 11,
          "pattern": "^db-[a-z0-9]{8}$",
          "format": "arch-db-id",
          "description": "Arch database ID.",
          "examples": ["db-abcdef12", "db-tarch230", "db-3me1ty44"]
        },
        {
          "type": "null"
        }
      ],
      "title": "Database For Loader"
    },
    "extractor_config": {
      "type": "string",
      "title": "extractor_config"
    },
    "loader_config": {
      "type": "string",
      "title": "loader_config"
    },
    "created_at": {
      "type": "string",
      "format": "date-time",
      "title": "Created At"
    },
    "updated_at": {
      "type": "string",
      "format": "date-time",
      "title": "Updated At"
    },
    "selection_mode": {
      "type": "string",
      "enum": ["include_default", "include_selected"],
      "title": "SelectionMode",
      "description": "Possible selection modes for a pipeline.\n\n`include_default` means that the `selected` field of PipelineTables and\nPipelineTableAttributes will be ignored, and all of the tap's default selections\nwill be used instead.\n\n`include_selected` means that the `selected` field of PipelineTables and\nPipelineTableAttributes will be respected."
    },
    "max_concurrent_syncs": {
      "type": "integer",
      "title": "Max Concurrent Syncs"
    },
