{
  "properties": {
    "id": {
      "type": "string",
      "maxLength": 23,
      "minLength": 23,
      "pattern": "^git_repository-[a-zA-Z0-9]{8}$",
      "format": "arch-git_repository-id",
      "title": "Id",
      "description": "Git repository ID.",
      "examples": [
        "git_repository-abcdef12",
        "git_repository-tarch230",
        "git_repository-3me1ty44"
      ]
    },
    "org_id": {
      "type": "string",
      "maxLength": 12,
      "minLength": 12,
      "pattern": "^org-[a-zA-Z0-9]{8}$",
      "format": "arch-org-id",
      "title": "Org ID",
      "description": "The ID of the parent organization.",
      "examples": [
        "org-abcdef12",
        "org-tarch230",
        "org-3me1ty44"
      ]
    },
    "url": {
      "type": "string",
      "title": "Url"
    }
  },
  "type": "object",
  "required": [
    "id",
    "url"
  ],
  "title": "GitRepository",
  "description": "Git repository schema."
}
