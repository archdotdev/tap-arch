{
  "properties": {
    "name": {
      "type": "string",
      "maxLength": 128,
      "minLength": 1,
      "title": "Name"
    },
    "description": {
      "type": "string",
      "maxLength": 2048,
      "title": "Description"
    },
    "id": {
      "type": "string",
      "maxLength": 16,
      "minLength": 16,
      "pattern": "^project-[a-zA-Z0-9]{8}$",
      "format": "arch-project-id",
      "title": "Id",
      "description": "Arch project ID.",
      "examples": [
        "project-abcdef12",
        "project-tarch230",
        "project-3me1ty44"
      ]
    },
    "org_id": {
      "type": "string",
      "maxLength": 12,
      "minLength": 12,
      "pattern": "^org-[a-zA-Z0-9]{8}$",
      "format": "arch-org-id",
      "title": "Org Id",
      "description": "Arch organization ID.",
      "examples": [
        "org-abcdef12",
        "org-tarch230",
        "org-3me1ty44"
      ]
    },
    "created_at": {
      "type": "string",
      "format": "date-time",
      "title": "Created At"
    },
    "updated_at": {
      "type": "string",
      "format": "date-time",
      "title": "Updated At"
    }
  },
  "type": "object",
  "required": [
    "name",
    "description",
    "id",
    "org_id",
    "created_at",
    "updated_at"
  ],
  "title": "Project",
  "description": "An Arch project."
}
